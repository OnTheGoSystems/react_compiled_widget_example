{
  "name": "react-ate-app-mini",
  "version": "0.0.1",
  "private": true,
  "jest": {
    "verbose": true,
    "moduleNameMapper": {
      "^root(.*)$": "<rootDir>app$1",
      "^global_root(.*)$": "<rootDir>$1"
    }
  },
  "scripts": {
    "build:test": "webpack --config webpack.config.js",
    "build:production": "webpack --mode=production --config webpack.config.js",
    "build:development": "webpack -w --config webpack.config.js",
    "test": "NODE_ENV=testing jest --no-cache"
