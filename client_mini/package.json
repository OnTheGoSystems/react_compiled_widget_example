    "@babel/preset-react": "",
    "babel-loader": "",
    "babel-preset-es2015": "",
    "jquery": "",
    "dateformat": "",
    "lodash": "",
    "react": "",
    "react-dom": "",
    "imports-loader": "",
    "webpack": ""
  },
  "devDependencies": {
    "babel-jest": "^16",
    "jest": "",
    "react-test-renderer": "",
    "webpack-cli": "^3.3.0",
    "webpack-command": "^0.4.2"
  }
